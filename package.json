{
	"name": "@opencode-ai/a2a",
	"version": "0.1.0",
	"description": "A2A Protocol plugin for OpenCode - enables communication with A2A-compatible agents",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"type": "module",
	"scripts": {
		"start": "node server.mjs",
		"dev": "node --watch server.mjs",
		"build": "tsc",
		"typecheck": "tsc --noEmit",
		"format": "biome format . --write",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --apply",
		"check": "biome check .",
		"prepublishOnly": "npm run typecheck && npm run build"
	},
	"dependencies": {
		"@a2a-js/sdk": "^0.3.10",
		"@opencode-ai/plugin": "^1.2.15",
		"uuid": "^9.0.0",
		"zod": "^3.22.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.4.4",
		"@types/express": "^4.17.21",
		"@types/node": "^20.10.0",
		"@types/uuid": "^9.0.7",
		"express": "^4.19.0",
		"typescript": "^5.9.3"
	},
	"keywords": [
		"opencode",
		"a2a",
		"agent",
		"protocol",
		"ai",
		"plugin"
	],
	"author": "",
	"license": "MIT"
}
